    background-color:#13aea1;
}
.op{opacity: 0}

/*电站点动画*/
@keyframes aniTwo {
    0% {
        transform: scale(0.1);
        opacity: 1;
    }
    100% {
        transform: scale(1.2);
        opacity: 0.8;
    }
}

@-webkit-keyframes aniTwo {
    0% {
        -webkit-transform: scale(0.1);
        opacity: 1;
    }
    100% {
        -webkit-transform: scale(1.2);
        opacity: 0.8;
    }
}







